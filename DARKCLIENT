dropdown:ChangeText("ALL") -- Args(<String> NewText)
dropdown:Toggle("ALL") -- Dropdowns and searchbars can go inside dropdowns

local label = section:Label("Player") -- Args(<String> LabelText, <Number?> TextSize, <Color3?> TextColor)
label:ChangeText("Player") -- Args(<String> NewText, <Boolean?> PlayAnimation)

local toggle = section:Toggle("GODMODE", function(bool)
   print("Toggle is: "..tostring(bool))
